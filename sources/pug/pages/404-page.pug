extends  ../_skeleton

//- Includes
include ../elements/_ui-parallax

block variables
  - var activeIndex = 5
  - var pageName = '404 page'

block page
  // Page header
  include ../sections/_header-default 


  +parallax({
    "src": "images/parallax-3.jpg",
    "class": 'parallax-header text-center'
  })
    .parallax-header__inner.context-dark: .parallax-header__content
      .container
        .row.justify-content-sm-center
          .col-md-10.col-xl-8
            .section-xxl
              h2 Page Not Found
              p The page requested couldn't be found - this could be due to a spelling error in the URL or a removed page.
              a(href='index.html').button.button-black.button-shadow Go back
  ="\n"

  // Page Footer
  include ../sections/_footer-corporate

